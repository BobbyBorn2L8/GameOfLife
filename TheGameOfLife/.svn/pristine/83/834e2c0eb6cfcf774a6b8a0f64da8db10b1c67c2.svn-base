package gameOfLife;

/**
 * @author Robert Kordula 40131366
 * Careers extends GameEntity so can be painted to a dialog box when the player is choosing careers
 * Class only contains getters and setters that are used for other classes
 */
public class Careers extends GameEntity
{
	/**
	 * 
	 */
	private static final long serialVersionUID = -1438622665506075561L;
	private String careerTitle;
	private int salary;
	
	public Careers(String imageName, String careerTitle, int salary)
	{
		super(0, 0, imageName);
		this.careerTitle = careerTitle;
		this.salary = salary;
	}

	//Getters and setters//
	public String getCareerTitle() {
		return careerTitle;
	}

	public void setCareerTitle(String careerTitle) {
		this.careerTitle = careerTitle;
	}

	public int getSalary() {
		return salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}	
}
